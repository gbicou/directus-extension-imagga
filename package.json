{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.3.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^10.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@bicou/semantic-release": "^1.0.1",
    "@directus/api": "^14.1.2",
    "@directus/extensions-sdk": "^10.2.0",
    "@directus/tsconfig": "^1.0.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "axios": "~1.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "form-data": "4.0.0",
    "prettier": "^3.1.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "semantic-release": "^22.0.8",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "@directus/api": "^13.1.0",
    "@directus/extensions-sdk": "^10.1.10",
    "axios": "~1.4.0",
    "form-data": "4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.11.0"
}
