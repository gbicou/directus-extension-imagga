  "devDependencies": {
    "@directus/sdk": "21.1.0",
    "@directus/tsconfig": "3.0.0",
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin": "5.9.0",
    "@types/node": "22.19.11",
    "dotenv": "17.3.1",
    "eslint": "9.39.2",
    "eslint-plugin-unicorn": "63.0.0",
    "rollup-plugin-node-externals": "8.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18",
    "wiremock-captain": "4.1.3"
  },
  "peerDependencies": {
    "@directus/api": "^33.0.0",
    "@directus/env": "^5.0.2",
    "@directus/extensions-sdk": "^17.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.30.2+sha512.36cdc707e7b7940a988c9c1ecf88d084f8514b5c3f085f53a2e244c2921d3b2545bc20dd4ebe1fc245feec463bb298aecea7a63ed1f7680b877dc6379d8d0cb4"
