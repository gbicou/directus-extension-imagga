{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.4.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^10.9.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "release": "changeset publish"
  },
  "dependencies": {
    "axios": "~1.6.7",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@directus/api": "^17.0.1",
    "@directus/env": "^1.0.2",
    "@directus/extensions-sdk": "^11.0.1",
    "@directus/tsconfig": "^1.0.1",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.2.5",
    "rollup-plugin-node-externals": "^7.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@directus/api": "^17.0.1",
    "@directus/extensions-sdk": "^11.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.15.1"
}
