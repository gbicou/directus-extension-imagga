    "@typescript-eslint/parser": "^5.57.1",
    "axios": "^1.3.5",
    "directus": "^9.24.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "prettier": "^2.8.7",
    "rollup-plugin-node-externals": "^5.1.2",
    "typescript": "^5.0.4"
