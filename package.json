{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.2.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^10.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@bicou/semantic-release": "^1.0.1",
    "@directus/api": "^10.1.0",
    "@directus/extensions-sdk": "^10.1.0",
    "@directus/tsconfig": "^0.0.7",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.8",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "axios": "~1.3.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "4.0.0",
    "lodash-es": "4.17.21",
    "prettier": "^2.8.8",
    "rollup-plugin-node-externals": "^6.0.0",
    "semantic-release": "^21.0.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@directus/api": "^10.0.0",
    "@directus/extensions-sdk": "^10.0.0",
    "axios": "~1.3.6",
    "form-data": "4.0.0",
    "lodash-es": "4.17.21"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.5.0"
}
