{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.6.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^11.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "release": "changeset publish",
    "test": "vitest run",
    "test:ci": "wait-on http://localhost:8055 && vitest run"
  },
  "dependencies": {
    "axios": "1.7.7",
    "form-data": "4.0.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.10",
    "@directus/api": "23.1.3",
    "@directus/sdk": "18.0.0",
    "@directus/env": "4.0.0",
    "@directus/extensions-sdk": "12.1.3",
    "@directus/tsconfig": "2.0.0",
    "@stylistic/eslint-plugin": "2.11.0",
    "@types/node": "20.17.6",
    "dotenv": "16.4.5",
    "eslint": "9.15.0",
    "eslint-plugin-unicorn": "56.0.1",
    "mockserver-client": "5.15.0",
    "rollup-plugin-node-externals": "7.1.3",
    "typescript": "5.6.3",
    "typescript-eslint": "8.15.0",
    "vitest": "2.1.5",
    "wait-on": "8.0.1"
  },
  "peerDependencies": {
    "@directus/api": "23.1.3",
    "@directus/env": "4.0.0",
    "@directus/extensions-sdk": "12.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387"
}
