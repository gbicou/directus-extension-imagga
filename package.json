{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.6.4",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^11.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@directus/api": "^26.0.0",
    "@directus/env": "^5.0.2",
    "@directus/extensions-sdk": "^13.0.3",
    "axios": "^1.7.9",
    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@directus/sdk": "19.1.0",
    "@directus/tsconfig": "3.0.0",
    "@eslint/js": "9.23.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/node": "22.14.0",
    "dotenv": "16.4.7",
    "eslint": "9.23.0",
    "eslint-plugin-unicorn": "58.0.0",
    "rollup-plugin-node-externals": "8.0.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vitest": "3.1.1",
    "wiremock-captain": "4.1.0"
  },
  "peerDependencies": {
    "@directus/api": "^25.0.1 || ^26.0.0",
    "@directus/env": "^5.0.2",
    "@directus/extensions-sdk": "^13.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "oracledb",
      "sqlite3"
    ],
    "onlyBuiltDependencies": [
      "argon2",
      "esbuild",
      "isolated-vm",
      "sharp",
      "vue-demi"
    ]
  }
}
