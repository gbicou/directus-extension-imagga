{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.2.2",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^10.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@bicou/semantic-release": "^1.0.1",
    "@directus/api": "^12.0.2",
    "@directus/extensions-sdk": "^10.1.5",
    "@directus/tsconfig": "^0.0.7",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "axios": "~1.4.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^48.0.0",
    "form-data": "4.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-node-externals": "^6.1.1",
    "semantic-release": "^21.0.7",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@directus/api": "^12.0.2",
    "@directus/extensions-sdk": "^10.1.5",
    "axios": "~1.4.0",
    "form-data": "4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.6"
}
