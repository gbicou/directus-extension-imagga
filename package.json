{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.6.5",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^11.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@directus/api": "^26.0.1",
    "@directus/env": "^5.0.3",
    "@directus/extensions-sdk": "^13.0.4",
    "axios": "^1.8.4",
    "form-data": "^4.0.2"
  },
  "devDependencies": {
    "@directus/sdk": "19.1.0",
    "@directus/tsconfig": "3.0.0",
    "@eslint/js": "9.24.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/node": "22.14.1",
    "dotenv": "16.5.0",
    "eslint": "9.24.0",
    "eslint-plugin-unicorn": "58.0.0",
    "rollup-plugin-node-externals": "8.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vitest": "3.1.2",
    "wiremock-captain": "4.1.0"
  },
  "peerDependencies": {
    "@directus/api": "^25.0.1 || ^26.0.0",
    "@directus/env": "^5.0.2",
    "@directus/extensions-sdk": "^13.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "oracledb",
      "sqlite3"
    ],
    "onlyBuiltDependencies": [
      "argon2",
      "esbuild",
      "isolated-vm",
      "sharp",
      "vue-demi"
    ]
  }
}
