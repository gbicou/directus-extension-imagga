{
  "name": "@bicou/directus-extension-imagga",
  "description": "Automatic image tagging of Directus files with Imagga API",
  "homepage": "https://github.com/gbicou/directus-extension-imagga",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-imagga",
  "icon": "extension",
  "version": "1.6.6",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image",
    "tag",
    "auto-tagging",
    "imagga"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.mjs",
    "source": "src/index.ts",
    "host": "^11.1.0"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@directus/api": "^27.0.2",
    "@directus/extensions-sdk": "^13.1.0",
    "axios": "^1.9.0",
    "form-data": "^4.0.2"
  },
  "devDependencies": {
    "@directus/sdk": "19.1.0",
    "@directus/tsconfig": "3.0.0",
    "@eslint/js": "9.28.0",
    "@stylistic/eslint-plugin": "4.4.0",
    "@types/node": "22.15.29",
    "dotenv": "16.5.0",
    "eslint": "9.28.0",
    "eslint-plugin-unicorn": "59.0.1",
    "rollup-plugin-node-externals": "8.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
    "vitest": "3.1.4",
    "wiremock-captain": "4.1.1"
  },
  "peerDependencies": {
    "@directus/api": "^25.0.1 || ^26.0.0 || ^27.0.0",
    "@directus/env": "^5.0.2",
    "@directus/extensions-sdk": "^13.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "oracledb",
      "sqlite3"
    ],
    "onlyBuiltDependencies": [
      "argon2",
      "esbuild",
      "isolated-vm",
      "sharp",
      "vue-demi"
    ]
  }
}
